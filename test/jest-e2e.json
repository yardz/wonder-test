{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@domain/(.*)": "<rootDir>/../src/domain/$1",
    "@modules/(.*)": "<rootDir>/../src/modules/$1",
    "@utils/(.*)": "<rootDir>/../src/utils/$1"
  }
}